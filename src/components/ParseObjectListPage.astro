---
import Page from './Page.astro';
import ParseObjectListPageScripts from './ParseObjectListPageScripts.astro';

export interface Props {
  title?: string;
  version?: string;
  jsFileName?: string;
}

const { title, version, jsFileName } = Astro.props;

// Basic page layout for ParseObjectList pages, e.g. pages/modules.astro
---

<Page title={title}>
  <slot name="head" slot="head" />
  <slot />

  {
    version && (
      <ParseObjectListPageScripts version={version} jsFileName={jsFileName} />
    )
  }
</Page>
