---
export interface Props {
  parseObjectClass: string;
  id: string;
  myVersions: string[];
  versionsData: Record<string, any>;
}

const { parseObjectClass, id, myVersions, versionsData } = Astro.props;

// Generate the base URL based on parseObjectClass
const baseUrl = `/WRFrontiersDB-Site/${parseObjectClass}/${id}`;
---

<h2>Available Versions</h2>
<ul>
  {myVersions.map((version: string) => (
    <li>
      <a href={`${baseUrl}/${version}`}>
        {versionsData[version]?.title || version} ({version})
      </a>
    </li>
  ))}
</ul>