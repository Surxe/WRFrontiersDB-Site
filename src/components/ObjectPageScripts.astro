---
export interface Props {
  version: string;
  objectData: any;
  jsFileName: string;
}

const { version, objectData, jsFileName } = Astro.props;
---

<script type="application/json" id="object-data" set:html={JSON.stringify(objectData)}></script>

<script type="module" define:vars={{ version }}>
  import { initializeLocalization } from '/WRFrontiersDB-Site/js/localization.js';
  initializeLocalization(version);
</script>

<script is:inline src={`/WRFrontiersDB-Site/js/${jsFileName}`}></script>